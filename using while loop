n_terms = int(input("How many terms in the Fibonacci sequence? "))

n1= 0 
n2 =1
count = 0

if n_terms <= 0:
   print("Please enter a valid positive integer")
elif n_terms == 1:
   print("Fibonacci sequence upto",n_terms,":")
   print(n1)
else:
   print("Fibonacci sequence:")
   while count < n_terms:
       print(n1)
       nth = n1 + n2
       n1 = n2
       n2 = nth
       count += 1
